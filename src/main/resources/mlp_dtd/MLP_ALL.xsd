            </xsd:sequence>
            <xsd:attribute name="srsName" type="xsd:string" use="optional"/>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ericsson_coord">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element ref="ericsson_X"/>
                <xsd:element ref="ericsson_Y"/>
            </xsd:sequence>
        </xsd:complexType>
