    <xsd:element name="ericsson_X" type="xsd:string"/>
    <xsd:element name="ericsson_Y" type="xsd:string"/>
    <xsd:element name="ericsson_distance" type="xsd:string"/>
    <xsd:element name="ericsson_recurrence_time" type="xsd:string"/>
