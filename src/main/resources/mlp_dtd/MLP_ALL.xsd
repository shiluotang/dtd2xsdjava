      ... 
  </svc_result>

Terms and conditions of use are available from the
Open Mobile Alliance Ltd. web site at
http://www.openmobilealliance.org/
-->
    <xsd:element name="shape">
        <xsd:complexType>
            <xsd:choice>
                <xsd:element ref="Point"/>
                <xsd:element ref="LineString"/>
                <xsd:element ref="Polygon"/>
                <xsd:element ref="Box"/>
                <xsd:element ref="CircularArea"/>
                <xsd:element ref="CircularArcArea"/>
                <xsd:element ref="EllipticalArea"/>
                <xsd:element ref="MultiLineString"/>
                <xsd:element ref="MultiPoint"/>
                <xsd:element ref="MultiPolygon"/>
                <xsd:element ref="LinearRing"/>
            </xsd:choice>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="distanceUnit" type="xsd:string"/>
    <xsd:element name="angularUnit" type="xsd:string"/>
    <xsd:element name="angle" type="xsd:string"/>
    <xsd:element name="coord">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element ref="X"/>
                <xsd:element maxOccurs="1" minOccurs="0" ref="Y"/>
                <xsd:element maxOccurs="1" minOccurs="0" ref="Z"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="X" type="xsd:string"/>
    <xsd:element name="Y" type="xsd:string"/>
    <xsd:element name="Z" type="xsd:string"/>
    <xsd:element name="Point">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element ref="coord"/>
            </xsd:sequence>
            <xsd:attribute name="gid" type="xsd:ID" use="optional"/>
            <xsd:attribute name="srsName" type="xsd:string" use="optional"/>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="LineString">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="2" ref="coord"/>
            </xsd:sequence>
            <xsd:attribute name="gid" type="xsd:ID" use="optional"/>
            <xsd:attribute name="srsName" type="xsd:string" use="optional"/>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="Box">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="2" minOccurs="2" ref="coord"/>
            </xsd:sequence>
            <xsd:attribute name="gid" type="xsd:ID" use="optional"/>
            <xsd:attribute name="srsName" type="xsd:string" use="optional"/>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="LinearRing">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="3" ref="coord"/>
            </xsd:sequence>
            <xsd:attribute name="gid" type="xsd:ID" use="optional"/>
            <xsd:attribute name="srsName" type="xsd:string" use="optional"/>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="Polygon">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element ref="outerBoundaryIs"/>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="innerBoundaryIs"/>
            </xsd:sequence>
            <xsd:attribute name="gid" type="xsd:ID" use="optional"/>
            <xsd:attribute name="srsName" type="xsd:string" use="optional"/>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="outerBoundaryIs">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element ref="LinearRing"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="innerBoundaryIs">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element ref="LinearRing"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="CircularArcArea">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element ref="coord"/>
                <xsd:element ref="inRadius"/>
                <xsd:element ref="outRadius"/>
                <xsd:element ref="startAngle"/>
                <xsd:element ref="stopAngle"/>
                <xsd:element maxOccurs="1" minOccurs="0" ref="angularUnit"/>
                <xsd:element maxOccurs="1" minOccurs="0" ref="distanceUnit"/>
            </xsd:sequence>
            <xsd:attribute name="gid" type="xsd:ID" use="optional"/>
            <xsd:attribute name="srsName" type="xsd:string" use="optional"/>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="CircularArea">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element ref="coord"/>
                <xsd:element ref="radius"/>
                <xsd:element maxOccurs="1" minOccurs="0" ref="distanceUnit"/>
            </xsd:sequence>
            <xsd:attribute name="gid" type="xsd:ID" use="optional"/>
            <xsd:attribute name="srsName" type="xsd:string" use="optional"/>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="EllipticalArea">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element ref="coord"/>
                <xsd:element ref="angle"/>
                <xsd:element ref="semiMajor"/>
                <xsd:element ref="semiMinor"/>
                <xsd:element maxOccurs="1" minOccurs="0" ref="angularUnit"/>
                <xsd:element maxOccurs="1" minOccurs="0" ref="distanceUnit"/>
            </xsd:sequence>
            <xsd:attribute name="gid" type="xsd:ID" use="optional"/>
            <xsd:attribute name="srsName" type="xsd:string" use="optional"/>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="inRadius" type="xsd:string"/>
    <xsd:element name="outRadius" type="xsd:string"/>
    <xsd:element name="radius" type="xsd:string"/>
    <xsd:element name="semiMajor" type="xsd:string"/>
    <xsd:element name="semiMinor" type="xsd:string"/>
    <xsd:element name="startAngle" type="xsd:string"/>
    <xsd:element name="stopAngle" type="xsd:string"/>
    <xsd:element name="MultiLineString">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="1" ref="LineString"/>
            </xsd:sequence>
            <xsd:attribute name="gid" type="xsd:ID" use="optional"/>
            <xsd:attribute name="srsName" type="xsd:string" use="optional"/>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="MultiPoint">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="1" ref="Point"/>
            </xsd:sequence>
            <xsd:attribute name="gid" type="xsd:ID" use="optional"/>
            <xsd:attribute name="srsName" type="xsd:string" use="optional"/>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="MultiPolygon">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:choice maxOccurs="unbounded" minOccurs="1">
                    <xsd:element ref="Polygon"/>
                    <xsd:element ref="Box"/>
                    <xsd:element ref="CircularArea"/>
                    <xsd:element ref="CircularArcArea"/>
                    <xsd:element ref="EllipticalArea"/>
                </xsd:choice>
            </xsd:sequence>
            <xsd:attribute name="gid" type="xsd:ID" use="optional"/>
            <xsd:attribute name="srsName" type="xsd:string" use="optional"/>
        </xsd:complexType>
    </xsd:element>
